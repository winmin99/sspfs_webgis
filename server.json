{
  "apps": {
    "name": "webgis",
    "cwd": "./",
    "script": "app.bundle.js",
    "node_args": "-r dotenv/config",
    "exec_mode": "cluster",
    "instances": 1,
    "watch": false,
    "max_memory_restart": "1G",
    "source_map_support": false,
    "combine_logs": true,
    "error_file": "./logs/pm2-error.log",
    "out_file": "/dev/null",
    "time": true,
    "wait_ready": false,
    "listen_timeout": 50000,
    "kill_timeout": 5000,
    "exp_backoff_restart_delay": 100,
    "autorestart": true,
    "env": {
      "NODE_ENV": "production"
    }
  }
}
